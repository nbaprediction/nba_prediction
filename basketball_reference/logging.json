{
    "version": 1,
    "formatters": {
            "simple": {
                "format": "%(asctime)s - %(process)d - [%(levelname)s] %(name)s - %(module)s.%(funcName)s: %(message)s"
            }
        },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "simple"
        }
    },
    "loggers": {
        "stringer-bell": {
            "handlers": ["console"],
            "level": "DEBUG"
        }
    }
}
